<% unless @members.blank? %>
  updateDynamicColumnContents('members', '<%= escape_javascript render(@members, group: @group, show_email: @show_emails, is_admin: (@current_user_is_admin || @badge_list_admin)) %>');
  $('#paginator-members').html('<%= escape_javascript(paginate(@members, remote: true, param_name: "pm").to_s) %>');
  checkForTooltips();
  checkForSpinnerLinks();
<% end %>

<% unless @badges.blank? %>
  updateDynamicColumnContents('badges', '<%= escape_javascript render(@badges, group: @group, requirements_map: @requirements_map, log_map: @log_map, is_member: @current_user_is_member, is_admin: @current_user_is_admin, expert_count_map: @expert_count_map, learner_count_map: @learner_count_map) %>');
  $('#paginator-badges').html('<%= escape_javascript(paginate(@badges, remote: true, param_name: "pb").to_s) %>');
<% end %>