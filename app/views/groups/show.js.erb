<% unless @members.blank? %>
  updateDynamicColumnContents('members', '<%= escape_javascript render(@members, group: @group, show_email: @show_emails, bl_admin_mode: @bl_admin_mode, is_admin: (@current_user_is_admin || @badge_list_admin)) %>');
  $('#paginator-members').html('<%= escape_javascript(paginate(@members, remote: true, param_name: "pm").to_s) %>');
  checkForTooltips();
  checkForSpinnerLinks();
<% end %>

<% unless @badges.blank? %>
  updateDynamicColumnContents('badges', '<%= escape_javascript render(@badges, group: @group, is_member: @current_user_is_member, is_admin: @current_user_is_admin) %>');
  $('#paginator-badges').html('<%= escape_javascript(paginate(@badges, remote: true, param_name: "pb").to_s) %>');
<% end %>